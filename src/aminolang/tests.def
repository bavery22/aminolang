class SimpleTest {
    func void run(Core core) {
        var stage:Stage;
        stage = core.createStage();
        var rect:Rect;
        rect = core.createRect();
        rect.setW(100);
        rect.setH(100);
        
        var circle:Circle;
        circle = core.createCircle();
        circle.setCx(200);
        circle.setCy(200);
        circle.setRadius(40);
        
        var c2:Circle;
        c2 = circle.setCx(300);
        
        var g:Group;
        g = core.createGroup();
        g.add(rect);
        g.add(circle);
        
        stage.setRoot(g);
    }
}
